{
  "name": "@otomadb/web",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "fmt": "pnpm run lint:prettier --write",
    "lint:eslint": "eslint . --ext \".js,.jsx,.ts,.tsx\"",
    "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,css,json,yml,md}\"",
    "lintfix": "run-s 'lintfix:*' 'fmt'",
    "lintfix:eslint": "pnpm run lint:eslint --fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "pnpm run codegen --watch",
    "schema:validate": "graphql-inspector validate \"./src/**/*.{ts,tsx}\" ./schema.graphql",
    "schema:diff": "graphql-inspector diff ./schema.graphql",
    "schema:introspect": "graphql-inspector introspect --write schema.graphql",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "storybook:preview": "serve storybook-static",
    "vercel:web": "run-s 'codegen' 'build'",
    "vercel:storybook": "run-s 'codegen' 'storybook:build'"
  },
  "engines": {
    "node": "^18"
  },
  "dependencies": {
    "@emotion/css": "^11.10.5",
    "@heroicons/react": "^2.0.13",
    "@hookform/resolvers": "3.0.0",
    "@marsidev/react-turnstile": "^0.1.0",
    "@urql/exchange-graphcache": "^5.0.8",
    "@vercel/og": "^0.4.0",
    "boring-avatars": "^1.7.0",
    "client-only": "^0.0.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "next": "13.2.4",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.7",
    "react-hot-toast": "^2.4.0",
    "react-use": "^17.4.0",
    "server-only": "^0.0.1",
    "urql": "^3.0.3",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-codegen/schema-ast": "^3.0.0",
    "@graphql-codegen/typescript-urql-graphcache": "^2.4.3",
    "@graphql-inspector/cli": "^3.4.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@storybook/addon-actions": "7.0.0-rc.5",
    "@storybook/addon-essentials": "7.0.0-rc.5",
    "@storybook/addon-interactions": "7.0.0-rc.5",
    "@storybook/addon-links": "7.0.0-rc.5",
    "@storybook/nextjs": "7.0.0-rc.5",
    "@storybook/react": "7.0.0-rc.5",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "autoprefixer": "10.4.14",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.3",
    "encoding": "^0.1.13",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-unused-imports": "2.0.0",
    "graphql-codegen-typescript-mock-data": "^3.0.0",
    "msw": "^1.0.0",
    "msw-storybook-addon": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.1",
    "serve": "^14.1.2",
    "storybook": "^7.0.0-beta.17",
    "style-loader": "^3.3.1",
    "tailwindcss": "3.3.0",
    "typescript": "4.9.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
